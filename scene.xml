<scenefile>
	<globaldata>
		<diffusecoeff v="0.5"/>
		<specularcoeff v="0.5"/>
		<ambientcoeff v="0.5"/>
		<transparentcoeff v="0.5"/>
	</globaldata>

	<cameradata>
		<pos x="0" y="0" z="0"/>
		<look x="0" y="0" z="-1"/>
		<up x="0" y="1" z="0"/>
		<heightangle v="45"/>
	</cameradata>

	<lightdata>
		<id v="1"/>
		<color r="1" g="1" b="1"/>
		<function v1="1.5" v2="0" v3="0"/>
		<type v="directional"/>
		<direction x="1" y="-1" z="-1"/>
	</lightdata>

	<object type="tree" name="root">
		<transblock>
			
			
			<object type="tree">
				<transblock>
					<scale x="10" y="10" z="10"/>
					<translate x="0" y="0" z="0"/>
					<object type="primitive" name="cube" >
						<diffuse r="0" g="0" b="1"/>
						<ambient r="0" g="1" b="1"/>
						<shininess v="25"/>
						<specular r="1" g="1" b="1"/>
						<transparent r="0.5" g="0.5" b="0.5"/>
					</object>
				</transblock>
				<transblock>
					<translate x="0" y="-5" z="0"/>
					<scale x="30" y="1" z="30"/>
					<object type="primitive" name="cube" >
						<diffuse r="0.7" g="0.6" b="0.1"/>
						<transparent r="1" g="1" b="1"/>
						
					</object>
				</transblock>
				<transblock>
					<translate x="3" y="4" z="0"/>
					<scale x="1" y="1" z="1"/>
					<object type="primitive" name="sphere" >
						<diffuse r="1" g="1" b="0"/>
						<transparent r="1" g="1" b="1"/>
						
					</object>
				</transblock>
				<transblock>
					<translate x="1" y="-2" z="-3"/>
					<scale x="1" y="1" z="1"/>
					<object type="primitive" name="cone" >
						<diffuse r="1" g="0" b="0"/>
						<transparent r="1" g="1" b="1"/>
						
					</object>
				</transblock>
				<transblock>
					<translate x="-2" y="1" z="2"/>
					<scale x="1" y="1" z="1"/>
					<object type="primitive" name="cylinder" >
						<diffuse r="0" g="1" b="1"/>
						<transparent r="1" g="1" b="1"/>
						
					</object>
				</transblock>
			</object>
		</transblock>
	</object>

</scenefile>

